{
  "name": "parcel-boilerplate",
  "version": "1.0.0",
  "description": "JS workflow with parcel",
  "main": "index.js",
  "scripts": {
    "dev": "npm run clean && parcel public/index.html --out-dir development -p 3000",
    "build": "parcel build public/index.html --out-dir dist --public-url ./",
    "clean": "rm -rf ./development && rm -rf ./.cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/genesisgabiola/parcel-boilerplate.git"
  },
  "keywords": [
    "parcel"
  ],
  "author": "Genesis Gabiola",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/genesisgabiola/parcel-boilerplate/issues"
  },
  "homepage": "https://github.com/genesisgabiola/parcel-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/runtime-corejs2": "^7.10.5",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.26.10"
  },
  "dependencies": {
    "axios": "^0.21.1"
  }
}
